{
  "name": "be-metamorphic",
  "version": "0.0.6",
  "description": "Use XSLT to turn a caterpillar of native HTML markup into a butterfly of web components-filled goodness",
  "main": "be-metamorphic.js",
  "module": "be-metamorphic.js",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "xslt"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node node_modules/xtal-test/serve.js",
    "package-check": "package-check"
  },
  "files": [
    "*.*"
  ],
  "exports": {
    ".": "./be-metamorphic.js",
    "./be-metamorphic.js": "./be-metamorphic.js"
  },
  "types": "types.d.ts",
  "dependencies": {
    "be-decorated": "0.0.48",
    "be-hive": "0.0.13",
    "trans-render": "0.0.433"
  },
  "devDependencies": {
    "xtal-test": "0.0.54",
    "@skypack/package-check": "0.2.2",
    "@custom-elements-manifest/analyzer": "0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-metamorphic.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-metamorphic/issues"
  },
  "homepage": "https://github.com/bahrus/be-metamorphic#readme"
}

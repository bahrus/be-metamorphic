  "exports": {
    ".": "./be-metamorphic.js",
    "./be-metamorphic.js": "./be-metamorphic.js"
  },
  "types": "types.d.ts",
  "dependencies": {
    "be-a-beacon": "0.0.6",
    "be-decorated": "0.0.147",
    "be-hive": "0.0.57",

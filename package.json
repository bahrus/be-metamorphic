    "@custom-elements-manifest/analyzer": "0.6.4",
    "@ui5/webcomponents": "1.6.0",
    "@ui5/webcomponents-fiori": "1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-metamorphic.git"
  },
